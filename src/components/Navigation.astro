---
const navItems = [
  { name: 'Home', url: '/' },
  { name: 'About', url: '/about' },
  { name: 'Curriculum', url: '/curriculum' },
  { name: 'Schedule', url: '/schedule' },
  { name: 'Resources', url: '/resources' },
  { name: 'Contact', url: '/contact' },
];
---

<nav>
  <ul>
    {navItems.map(item => (
      <li><a href={item.url}>{item.name}</a></li>
    ))}
  </ul>
</nav>
